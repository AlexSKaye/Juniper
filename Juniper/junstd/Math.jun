module Math
open(Prelude)

let pi : float = 3.141592653589793238462643383279502884197169399375105820974
let e : float = 2.718281828459045235360287471352662497757247093699959574966

fun degToRad(degrees : float) : float =
    degrees * 0.017453292519943295769236907684886

fun radToDeg(radians : float) : float =
    radians * 57.295779513082320876798154814105

fun acos_(x : float) : float = (
    let ret = 0.0;
    #ret = acos(x);#;
    ret
)

fun asin_(x : float) : float = (
    let ret = 0.0;
    #ret = asin(x);#;
    ret
)

fun atan_(x : float) : float = (
    let ret = 0.0;
    #ret = atan(x);#;
    ret
)

fun atan2_(y: float, x : float) : float = (
    let ret = 0.0;
    #ret = atan(y, x);#;
    ret
)

fun cos_(x : float) : float = (
    let ret = 0.0;
    #ret = cos(x);#;
    ret
)

fun cosh_(x : float) : float = (
    let ret = 0.0;
    #ret = cosh(x);#;
    ret
)

fun sin_(x : float) : float = (
    let ret = 0.0;
    #ret = sin(x);#;
    ret
)

fun sinh_(x : float) : float = (
    let ret = 0.0;
    #ret = sinh(x);#;
    ret
)

fun tanh_(x : float) : float = (
    let ret = 0.0;
    #ret = tanh(x);#;
    ret
)

fun exp_(x : float) : float = (
    let ret = 0.0;
    #ret = exp(x);#;
    ret
)

fun frexp_(x : float) : (float * uint16) = (
    let ret = 0.0;
    let exponent : uint16 = 0;
    #ret = frexp(x, &exponent);#;
    (ret, exponent)
)

fun ldexp_(x : float, exponent : uint16) : float = (
    let ret = 0.0;
    #ret = ldexp(x, exponent);#;
    ret
)

fun ln(x : float) : float = (
    let ret = 0.0;
    #ret = log(x);#;
    ret
)

fun log10_(x : float) : float = (
    let ret = 0.0;
    #ret = log10(x);#;
    ret
)

fun modf_(x : float) : (float * float) = (
    let ret = 0.0;
    let integer = 0.0;
    #ret = modf(x, &integer);#;
    (ret, integer)
)

fun pow_(x : float, y : float) : float = (
    let ret = 0.0;
    #ret = pow(x, y);#;
    ret
)

fun sqrt_(x : float) : float = (
    let ret = 0.0;
    #ret = sqrt(x);#;
