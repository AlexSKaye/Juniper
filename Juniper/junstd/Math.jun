)

fun ceil_(x : float) : float = (
    let ret = 0.0;
    #ret = ceil(x);#;
    ret
)

fun abs(x : float) : float = (
    let ret = 0.0;
    #ret = fabs(x);#;
    ret
)

fun floor_(x : float) : float = (
    let ret = 0.0;
    #ret = floor(x);#;
    ret
)

fun fmod_(x : float, y : float) : float = (
    let ret = 0.0;
    #ret = fmod(x, y);#;
    ret
)